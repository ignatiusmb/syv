{
	"name": "syv",
	"version": "0.0.46",
	"description": "practical svelte components and utilities",
	"repository": "github:ignatiusmb/syv",
	"author": "Ignatius Bagus",
	"license": "MIT",
	"type": "module",
	"scripts": {
		"reset": "git reset --hard && git clean -dfx -e node_modules",
		"dev": "vite dev",
		"build": "vite build",
		"format": "prettier --write .",
		"check": "pnpm run \"/^check:.*/\"",
		"check:style": "prettier --check .",
		"check:svelte": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"package": "svelte-package && pnpm copy",
		"copy": "cp package.json README.md LICENSE dist/"
	},
	"module": "./index.js",
	"types": "./index.d.ts",
	"exports": {
		".": {
			"types": "./index.d.ts",
			"svelte": "./index.js"
		},
		"./core/*.svelte": {
			"types": "./core/*.svelte.d.ts",
			"svelte": "./core/*.svelte"
		},
		"./attachment": {
			"types": "./attachment/index.d.ts",
			"default": "./attachment/index.js"
		},
		"./internal/*.svelte": {
			"types": "./internal/*.svelte.d.ts",
			"svelte": "./internal/*.svelte"
		},
		"./loader/*.svelte": {
			"types": "./loader/*.svelte.d.ts",
			"svelte": "./loader/*.svelte"
		},
		"./options": {
			"types": "./options.d.ts",
			"default": "./options.js"
		},
		"./types": {
			"types": "./types.d.ts"
		},
		"./utils": {
			"types": "./utils.d.ts",
			"default": "./utils.js"
		}
	},
	"packageManager": "pnpm@10.12.1",
	"prettier": "mauss/prettier.config.js",
	"keywords": [
		"svelte",
		"sveltekit",
		"headless",
		"library",
		"components",
		"svelte-components",
		"actions",
		"attachments",
		"utilities",
		"toolbox",
		"ssr-safe",
		"typescript",
		"progressive-enhancement",
		"frontend"
	],
	"peerDependencies": {
		"svelte": "^5.0.0"
	},
	"devDependencies": {
		"@ignatiusmb/styles": "^0.1.1",
		"@sveltejs/adapter-vercel": "^5.7.2",
		"@sveltejs/kit": "^2.21.1",
		"@sveltejs/package": "^2.3.11",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"aubade": "^0.9.1",
		"mauss": "^0.9.1",
		"prettier": "^3.5.3",
		"prettier-plugin-sort-package-json": "^1.0.1",
		"prettier-plugin-svelte": "^3.4.0",
		"svelte": "^5.33.13",
		"svelte-check": "^4.2.1",
		"svelte2tsx": "^0.7.39",
		"typescript": "^5.8.3",
		"vite": "^6.3.5"
	}
}
